{
    "extends": "airbnb",
    "installedESLint": true,
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "rules": {
      "prefer-stateless-function": [ 0, { "ignorePureComponents": true }],
      "no-console": 0,
      "max-len": ["error", {
        "ignoreComments": true,
        "ignoreUrls": true,
        "ignoreTemplateLiterals": true,
        "ignoreStrings": true
      }]
    }
}
